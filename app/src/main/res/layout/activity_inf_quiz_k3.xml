        android:textSize="20dp"
        app:layout_constraintBottom_toTopOf="@+id/ivInfQuizk3prostokat"
        app:layout_constraintStart_toStartOf="@+id/ivInfQuizk3prostokat"
        app:layout_constraintTop_toBottomOf="@+id/ivInfQuizk3prostokat"
        tools:visibility="gone" />

    <TextView
        android:id="@+id/tvInfQuizK3TrojkatWysokosc"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="56dp"
        android:layout_marginTop="62dp"
        android:text="12"
        android:textSize="20dp"
        app:layout_constraintStart_toStartOf="@+id/ivInfQuizk3Trojkat"
        app:layout_constraintTop_toTopOf="@+id/ivInfQuizk3Trojkat"
        tools:visibility="gone" />

    <TextView
        android:id="@+id/tvInfQuizK3TrojkatPodstawa"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="13dp"
        android:text="12"
        android:textSize="20dp"
        app:layout_constraintBottom_toBottomOf="@+id/ivInfQuizk3Trojkat"
        app:layout_constraintEnd_toStartOf="@+id/ivInfQuizk3Trojkat"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/ivInfQuizk3Trojkat"
        tools:visibility="gone" />

    <TextView
        android:id="@+id/tvInfQuizK3KoloPromien"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="50dp"
        android:layout_marginEnd="35dp"
        android:text="12"
        android:textSize="20dp"
        app:layout_constraintEnd_toEndOf="@+id/ivInfQuizk3kolo"
        app:layout_constraintTop_toTopOf="@+id/ivInfQuizk3kolo"
        tools:visibility="gone" />

    <TextView
        android:id="@+id/tvInfQuizK3TrojkatBok2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="47dp"
        android:layout_marginEnd="35dp"
        android:text="12"
        android:textSize="20dp"
        app:layout_constraintEnd_toEndOf="@+id/ivInfQuizk3Trojkat"
        app:layout_constraintTop_toTopOf="@+id/ivInfQuizk3Trojkat"
        tools:visibility="gone" />

    <TextView
        android:id="@+id/tvInfQuizK3TrojkatBok3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
