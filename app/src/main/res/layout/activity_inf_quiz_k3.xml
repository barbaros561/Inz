        android:textSize="20dp"
        app:layout_constraintBottom_toTopOf="@+id/ivInfQuizk3kwadrat"
        app:layout_constraintStart_toStartOf="@+id/ivInfQuizk3kwadrat"
        app:layout_constraintTop_toBottomOf="@+id/ivInfQuizk3kwadrat"
        tools:visibility="gone" />

    <TextView
        android:id="@+id/tvInfQuizK3ProstokatTop"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
