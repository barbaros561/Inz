        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="-1dp"
        android:layout_marginBottom="130dp"
        android:text="1"
        android:textSize="20dp"
        app:layout_constraintBottom_toBottomOf="@+id/ivInfQuizk3kwadrat"
        app:layout_constraintEnd_toStartOf="@+id/ivInfQuizk3kwadrat"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/ivInfQuizk3kwadrat"
        app:layout_constraintTop_toTopOf="@+id/ivInfQuizk3kwadrat"
        tools:visibility="gone" />

